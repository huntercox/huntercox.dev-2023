{
	"name": "hcox/huntercox.dev-theme-2023",
	"type": "project",
	"require": {
		"php": ">=8.1.9",
		"composer/installers": "^1.11",
		"ffraenz/private-composer-installer": "^5.0",
		"wpackagist-plugin/classic-editor": "^1.6.3",
		"advanced-custom-fields/advanced-custom-fields-pro": "*",
		"huntercox/huntercox.dev-theme-2023": "dev-main"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "advanced-custom-fields/advanced-custom-fields-pro",
				"version": "6.1.4",
				"type": "wordpress-plugin",
				"dist": {
					"type": "zip",
					"url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%PLUGIN_ACF_KEY}&t={%VERSION}"
				}
			}
		},
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "vcs",
			"url": "https://github.com/huntercox/huntercox.dev-theme-2023"
		}
	],
	"extra": {
		"dropin-paths": {
			"wp-content/": [
				"package:aaemnnosttv/wp-sqlite-db:src/db.php"
			]
		},
		"installer-paths": {
			"wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		},
		"private-composer-installer": {
			"dotenv-path": ".",
			"dotenv-name": ".env"
		}
	},
	"license": "GPL-2.0+",
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"ffraenz/private-composer-installer": true
		},
		"preferred-install": "dist",
		"platform": {
			"php": "8.1.9"
		},
		"sort-packages": true
	}
}